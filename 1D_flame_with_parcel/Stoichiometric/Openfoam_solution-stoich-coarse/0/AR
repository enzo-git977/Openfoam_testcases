FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      AR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 2.89e-05;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 2.89e-05;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


