FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0]; // [ M L T I Î˜ N J ]

internalField   uniform (0.3779 0 0); //laminar flame speed

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0.3779 0 0); //laminar flame speed
    }

    outlet
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

